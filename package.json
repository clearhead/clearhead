      "email": "beaulm@gmail.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.19",
    "babel-core": "^5.8.22",
    "babel-plugin-object-assign": "^1.2.1",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "chai": "^3.2.0",
    "del": "^1.2.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
