    "mocha": "^2.2.5",
    "mocha-jsdom": "^1.0.0",
    "phantomjs": "^1.9.18",
    "sinon": "^1.15.4",
    "through2": "^2.0.0"
  },
  "dependencies": {
    "karma-es6-shim": "^0.1.3"
  }
